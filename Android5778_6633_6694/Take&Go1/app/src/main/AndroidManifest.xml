<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yealm.takego1">


    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".controller.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".controller.AddCostumer"
            android:label="Add Costumer" />
        <activity android:name=".controller.AddCarModel" />
        <activity android:name=".controller.AddCar" />
        <activity android:name=".controller.ShowAllCostumers" />
        <activity android:name=".controller.ShowAllCars" />
        <activity android:name=".controller.ShowAllCarModel" />
        <activity android:name=".controller.ShowAllBranch"></activity>
    </application>

</manifest>